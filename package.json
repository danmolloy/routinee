    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.11",
    "formik": "^2.4.2",
    "jest": "^29.2.1",
    "jest-expo": "^49.0.0",
    "luxon": "^3.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-animatable": "^1.3.3",
    "react-native-calendars": "^1.1300.0",
    "react-native-gesture-handler": "~2.12.0",
