  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react-native": "^12.3.0",
    "@types/luxon": "^3.3.1",
    "@types/react": "~18.2.14",
    "@types/react-test-renderer": "^18.0.1",
    "react-test-renderer": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
